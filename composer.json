{
  "name": "kuriousagency/commerce-currency-prices",
  "description": "Adds payment currency prices to products",
  "type": "craft-plugin",
  "version": "2.7.4",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "currency prices"
  ],
  "support": {
    "docs": "https://github.com/webdna/commerce-currency-prices/currency-prices/blob/master/README.md",
    "issues": "https://github.com/webdna/commerce-currency-prices/currency-prices/issues"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "WebDNA",
      "homepage": "https://webdna.co.uk"
    }
  ],
  "require": {
    "craftcms/cms": "^3.5.0",
    "craftcms/commerce": "^3.2.0"
  },
  "autoload": {
    "psr-4": {
      "kuriousagency\\commerce\\currencyprices\\": "src/"
    }
  },
  "extra": {
    "name": "Commerce Currency Prices",
    "handle": "commerce-currency-prices",
    "hasCpSettings": false,
    "hasCpSection": false,
    "changelogUrl": "https://raw.githubusercontent.com/webdna/commerce-currency-prices/currency-prices/master/CHANGELOG.md",
    "components": {
      "currencyPricesService": "kuriousagency\\commerce\\currencyprices\\services\\CurrencyPricesService"
    },
    "class": "kuriousagency\\commerce\\currencyprices\\CurrencyPrices"
  }
}
