{
	"name": "kuriousagency/commerce-currency-prices",
	"description": "Adds payment currency prices to products",
	"type": "craft-plugin",
	"version": "2.1.0",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"currency prices"
	],
	"support": {
		"docs": "https://github.com/KuriousAgency/commerce-currency-prices/currency-prices/blob/master/README.md",
		"issues": "https://github.com/KuriousAgency/commerce-currency-prices/currency-prices/issues"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Kurious Agency",
			"homepage": "https://kurious.agency"
		}
	],
	"require": {
		"craftcms/cms": "^3.0.0-RC1"
	},
	"autoload": {
		"psr-4": {
			"kuriousagency\\commerce\\currencyprices\\": "src/"
		}
	},
	"extra": {
		"name": "Currency Prices",
		"handle": "currency-prices",
		"hasCpSettings": false,
		"hasCpSection": false,
		"changelogUrl": "https://raw.githubusercontent.com/KuriousAgency/commerce-currency-prices/currency-prices/master/CHANGELOG.md",
		"components": {
			"currencyPricesService": "kuriousagency\\commerce\\currencyprices\\services\\CurrencyPricesService"
		},
		"class": "kuriousagency\\commerce\\currencyprices\\CurrencyPrices"
	}
}
